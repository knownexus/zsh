alias g!='git status'
alias gdf='git diff'
alias gdfs='git diff HEAD~1'
alias git!='git status'
alias gita='git add'
alias gitb='git branch'
alias gitc='git checkout'
alias gitcb='git checkout -b'
alias gitca='git commit --amend'
alias gitcm='git commit -m'
alias gitfp='git push --force'
alias gitl='git log'
alias gitlg="git log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all"
alias gitlg2='git log --graph --abbrev-commit --decorate --format=format:'\''%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n'\'''\''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)'\'' --all'
alias gitpf='git push --force'
alias gitra='git rebase --abort'
alias gitrc='git rebase --continue'
alias gitres='git checkout --'
alias gits='git show'
alias giturl='git config --get remote.origin.url'
alias gl='git log'
alias gp='git pull'
alias gpf='git push --force'
alias gres='git checkout --'
alias gress='git restore --staged'
alias grd='git rebase develop'
alias gri='f(){ git rebase -i HEAD~$1; }; f'
alias gro='git rebase origin'
alias groi='git rebase origin -i'
alias grod='git rebase origin/develop'
alias gru='git remote update'
alias grm='git rebase master'
alias gs='git show'
alias gup='git reset HEAD~1'
alias rebase='f() { branch=$(git reflog show --all | grep $1 | awk '\''/refs/'\'' | awk '\''NR==1{print $2}'\'' | sed -e "s/refs\/remotes\///" | sed "s/\@.*//"); git rebase $branch; }; f'
alias remote!='branch=$(git rev-parse --abbrev-ref HEAD); git reset --hard origin/$branch'
alias rootb='branch=$(git reflog show --all | grep $1 | awk '\''/refs/'\'' | awk '\''NR==1{print $2}'\'' | sed -e "s/refs\/remotes\///" | sed "s/\@.*//")'
alias stash='git stash save'
alias stashp='git stash pop'
alias uncommit='git reset --soft HEAD^'
alias update='git remote update && git rebase origin'
alias updated='git remote update && git rebase origin/develop'