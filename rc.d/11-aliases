# -*- sh -*-

# Configure stuff as I like it.

alias bye='exit'
alias g='grep -rniI . -e'
alias md='mkdir'
alias ng='grep -v'
alias r='reset'
alias remspace='sed -i '"'"'s/[[:space:]]*$//'"'"' *'
alias rmr='rm -r'

# CD alaises
alias cdh='cd ~'
alias rp='cd ~/source/repos/voiceman'
alias rpv='cd ~/source/repos/voiceman/Voice'
alias rpw='cd ~/source/repos/voiceman/Voiceman.Web'
alias rpwa='cd ~/source/repos/voiceman/Voiceman.Web/Voiceman.AngularUI'
alias rp2='cd ~/source/repos/voiceman2'
alias rp2v='cd ~/source/repos/voiceman2/Voice'
alias rp2w='cd ~/source/repos/voiceman2/Voiceman.Web'
alias rp2wa='cd ~/source/repos/voiceman2/Voiceman.Web/Voiceman.AngularUI'
alias rpfp='cd ~/source/repos/voiceman.FileProcessor'

alias v='vim'
alias vb='vim ~/.resources/zsh/rc.d'
alias sb='source ~/.resources/zsh/rc'

