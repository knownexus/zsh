# -*- sh -*-

# Configure stuff as I like it.

alias bye='exit'
alias cdh='cd ~'
alias g='grep -rniI . -e'
alias md='mkdir'
alias ng='grep -v'
alias r='reset'
alias remspace='sed -i '"'"'s/[[:space:]]*$//'"'"' *'
alias rmr='rm -r'
alias v='vim'
alias vb='vim ~/.resources/zsh/rc.d'
alias sb='source ~/.resources/zsh/rc'
